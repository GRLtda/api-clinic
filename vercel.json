{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "functions": {
    "src/server.js": {
      "maxDuration": 60,
      "includeFiles": "node_modules/@sparticuz/chromium/bin/**",
      "external": [
        "@sparticuz/chromium"
      ]
    }
  }
}